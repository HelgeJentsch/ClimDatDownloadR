@article{lindauJointInfluenceBreak2018,
  title = {The Joint Influence of Break and Noise Variance on the Break Detection Capability in Time Series Homogenization},
  author = {Lindau, Ralf and Venema, Victor Karel Christiaan},
  year = {2018},
  month = aug,
  journal = {Advances in Statistical Climatology, Meteorology and Oceanography},
