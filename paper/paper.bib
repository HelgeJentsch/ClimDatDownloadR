  editor = {Swaroop, Abhishek and Polkowski, Zdzislaw and Correia, S{\'e}rgio Duarte and Virdee, Bal},
  year = {2024},
  pages = {173--188},
  publisher = {Springer Nature Singapore},
  address = {Singapore},
  abstract = {Identification of the right abiotic factors for an ambient environment, soil, and water is critical for successful agricultural activities. However, these factors have been influenced by climate change, and other factors as such are no longer in their natural state of equilibrium. This has affected the ecosystem and has made previously suitable environments currently unsuitable for most organisms and crop production. To have production of healthy crops with high yield and quality, precise agricultural inputs are required, including the right environmental abiotic factors. This goal has been achieved to some extent via extensive studies carried out by various scholars to ascertain their impact and possible mitigating action. In this paper, we first investigated the factors that influence the environment adversely and the application of SDM as a solution depicted in other studies and further presented an overview of some of the SDM-based software packages and their applications, with much emphasis on agronomy. This review aims to provide insight into, how effective computer-based SDM has been or otherwise in resolving the challenges in the agriculture sector and the future direction for the application of SDM-based solutions.},
  isbn = {978-981-99-6547-2}
}

